pragma solidity   ^0.5.0;
contract tech4dev{

//view parameter//
    uint public number;
    function add() public view returns (uint){
        return  number;
    }
//pure parameter//
     function Halimat(uint x, uint y) public pure returns(uint){

         return x+y;
     }

     function subtract() public  view returns(uint){
         return number +2; 


     }

 }
}
