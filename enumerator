/*classwork: Create and enum for the following pending, recieved , rejected, create a function to update another 
 function to get the enum*/
enum Shipping {
        Pending,
        Received,
        Accepted,
        Rejected,
        Canceled
    }
 Shipping public Status;
function Set(Shipping  _x) public {
    Status = _x;
 }

function update() public{
    
}

function Get() public view returns(Shipping){
return Status;

}
function Pending() public {
    Status = Shipping.Pending;
}

function Received() public {
    Status = Shipping.Received;

}
function Accepted() public {
    Status = Shipping.Accepted;


}
function Rejected() public {
    Status = Shipping.Rejected;

}

function Canceled() public {
    Status = Shipping.Canceled;

}
}
